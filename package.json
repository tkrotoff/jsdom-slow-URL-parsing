{
  "name": "jsdom-slow-url-parsing",
  "version": "0.0.0",
  "description": "jsdom slow URL parsing",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean:all": "rm -rf node_modules package-lock.json",
    "npm:update": "npm run clean:all && npx npm-check-updates --upgrade && npm install",
    "patch:whatwg-url": "cd node_modules/whatwg-url && patch -p1 < ../../patches/whatwg-url+14.2.0.patch",
    "debug": "node --cpu-prof ./node_modules/.bin/vitest run",
    "test": "vitest run",
    "tsc": "tsc",
    "format": "prettier --write .",
    "lint": "npm run tsc"
  },
  "devDependencies": {
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
